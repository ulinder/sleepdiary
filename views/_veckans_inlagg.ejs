<h2>Veckans inlägg</h2>

<template id="template-veckans-inlagg">
  <div class="card card-inlagg inlagg-expanded">
      <div class="card-body">

            <div class="inlagg-flex">
                <span class="inlagg-datum">Måndag 2 mars</span>
              </div>

            <div class="inlagg-flex">

                <div class="col-4.4">
                  <span class="inlagg-description"> Sänggång </span> <br />
                  <span class="inlagg-down">HH:MM</span>
                </div>

                <div class="col-4.3">
                  <span class="inlagg-description"> Klev upp </span> <br>
                  <span class="inlagg-up">HH:MM</span>
                </div>

                <div class="col-3.3">
                  <span class="inlagg-description"> Skattning </span> <br>
                  <span class="inlagg-rate"><p class="stars">★★★</p></span>
                </div>
            </div>


        <div class="inlagg-flex">
                <div class="col-4.1">
                  <span class="inlagg-description"> Vaken i säng </span> <br>
                  <span class="inlagg-awake">Hh Mmin</span>
                </div>

              <div class="col-3.9">
                <span class="inlagg-description"> Sovtid </span> <br>
                <span class="inlagg-asleep">Hh Mmin</span>
              </div>
                  <div class="col-4">
                  </div>
        </div>

      <div class="inlagg-flex">
              <div class="col-8">
                <span class="inlagg-description">Sömneffektivitet </span> <br>
                <span class="inlagg-efficiency">XX%</span>
              </div>
      </div>

            <div class="inlagg-flex">
              <div class="flex-kolumn">
                  <a class="edit-post-link" href="/">
                  <button class="btn btn-primary btn-redigera"><i class="fa fa-pencil"></i> Redigera </button>
                  </a>
                </div>

          <!--    <div class="kortinnehall-knapp">
                <button class="btn btn-primary btn-expand"> <i class="fa fa-chevron-down"></i> </button>
               </div>
           -->
                </div>

        </div>

    </div>

</template>


