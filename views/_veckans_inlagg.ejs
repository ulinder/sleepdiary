
    <div class="card card-inlagg">
        <div class="card-body">

              <div class="inlagg-flex">
                    <div class="flex-kolumn">
                          <div>
                             <span class="inlagg-datum"></span>
                           </div>
                        
                          <div class="flex-rad">
                              <div class="flexElement">
                                    <span class="inlagg-description"> Sänggång </span>
                                    <span class="inlagg-down"></span>
                                </div>
                              <div class="flexElement">
                                    <span class="inlagg-description"> Klev upp </span>
                                    <span class="inlagg-up"></span>
                              </div>
                          </div>
          
                     </div>
                    <div class="kortinnehall-knapp">
                      <button class="btn btn-primary btn-expand"> <i class="fa fa-chevron-down"></i> </button>

                     </div>
            </div>

        </div>
      </div>




<% data_table.forEach( (day)=> { %> 

  <div class="card card-inlagg inlagg-expanded">
      <div class="card-body">

            <div class="inlagg-flex">
               <div class="flex-kolumn">
                          <span class="inlagg-datum "><%= day.day %></span>
                          <% if(day.time_up_from_bed){ %>

                          <span class="inlagg-description"> Sänggång </span>
                          <span class="inlagg-down"><%= day.time_to_bed.split(" ")[1] %></span>

                          <span class="inlagg-description"> Vaken i säng </span>
                          <span class="inlagg-awake"><%= day.seconds_awake %></span>

                          <span class="inlagg-description"> Sömneffektivitet </span>
                          <span class="inlagg-efficiency"><%= day.sleep_efficiency %>%</span>
                          <% } %>
                </div>
              
              <% if(day.time_up_from_bed){ %>

              <div class="flex-kolumn">
                          <span class="inlagg-description"> Klev upp </span>
                          <span class="inlagg-up"><%= day.time_up_from_bed.split(" ")[1] %></span>

                          <span class="inlagg-description"> Sovtid </span>
                          <span class="inlagg-asleep"><%= day.seconds_asleep %></span>
                </div>

              <div class="flex-kolumn">
                          <span class="inlagg-description"> Skattning </span>
                          <span class="inlagg-rate"><%=day.sleep_rate %></span>
                </div>
              <% } %> <!-- End if -->
          </div>
        </div>

    </div>
<% }); %>


