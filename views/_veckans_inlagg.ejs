<template>
  <div class="card card-inlagg inlagg-expanded <%=addClass%>">
      <div class="card-body">

            <div class="inlagg-flex">
               <div class="flex-kolumn">
                          <span class="inlagg-datum ">YYYY-MM-DD</span>
                          

                          <span class="inlagg-description"> Sänggång </span>
