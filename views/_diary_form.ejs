
<div class="card">
    <div class="card-body">

<form action="posts" class="" method="post">

  <div class="mb-4 row">
    <div class="col">
      <button type="button" class=" info">!</button>
      <label class="question-label" for="date">Datum när jag lade mig i sängen</label>

      <input required=required type="date" class="form-control blue-input-frame" id="morning_date" name="down_date" onchange="check_form();" value="<%= moment().subtract(1, 'days').format("YYYY-MM-DD") %>"  class="form-control">
    </div>
  </div>

<hr class="linje-ny-dagbok">


  <div class="disabler">

      <div class="mb-4 clockpicker input-group row" data-autoclose="true">
        <div class="col">
          <button type="button" class=" info">!</button>
          <label class="question-label" for="sleep_time">Klockslag jag lade mig i sängen</label>
          <input type="text" class="form-control blue-input-frame liten-ruta"  name="down_time" placeholder="00:00">
          <span class="input-group-addon">
              <span class="glyphicon glyphicon-time"></span>
          </span>
        </div>
      </div>
<hr class="linje-ny-dagbok">

      <div class="mb-4 row">
        <div class="col ">
            <button type="button" class=" info">!</button>



            <label class="question-label flex-column-center" for="awake">Total tid jag låg vaken i sängen</label>

          <!--  <input type="text" name="slider-target" class="blue-input-frame" id="awake-slider-target" value="0">
            <input required=required type="range" min=0 max=1080 step="10" name="awake" id="awake-slider" class="form-range" value=0>
            -->

          </div>
        </div>

        <div class="mb-4 row">
              <div class="tid-dropdown">
                      <div>
                      <select class=" blue-input-frame " aria-label="Default select example">
                        <option selected>0 h</option>
                        <option value="60">1 h</option>
                        <option value="120">2 h</option>
                        <option value="180">3 h</option>
                        <option value="240">4 h</option>
                        <option value="300">5 h</option>
                        <option value="360">6 h</option>
                        <option value="420">7 h</option>
                        <option value="480">8 h</option>
                        <option value="540">9 h</option>
                        <option value="600">10+ h</option>
                      </select>
                      </div>
                      <div>
                      <select class=" blue-input-frame " aria-label="Default select example">
                        <option selected>0 min</option>
                        <option value="10">10 min</option>
                        <option value="20">20 min</option>
                        <option value="30">30 min</option>
                        <option value="40">40 min</option>
                        <option value="50">50 min</option>
                      </select>
                      </div>

                </div>
        </div>




<hr class="linje-ny-dagbok">

  <div class="mb-4 row">
    <div class="col">
      <button type="button" class=" info">!</button>
      <label class="question-label" for="date">Datum när jag klev upp ur sängen</label>
      <input required=required type="date" class="form-control blue-input-frame" id="morning_date" name="up_date" onchange="check_form();" value="<%= moment().format("YYYY-MM-DD") %>"  class="form-control">
    </div>
  </div>

<hr class="linje-ny-dagbok">

      <div class="mb-4 clockpicker row" data-autoclose="true">
        <div class="col">
          <button type="button" class=" info">!</button>
          <label class="question-label" for="sleep_time">Klockslag jag klev upp ur sängen</label>
          <input type="text" class="form-control blue-input-frame liten-ruta" name="up_time" id="up" placeholder="00:00">
          <span class="input-group-addon">
              <span class="glyphicon glyphicon-time"></span>
          </span>
        </div>
      </div>

<hr class="linje-ny-dagbok">

      <div class="mb-4 row">
        <div class="col stars">
          <button type="button" class=" info">!</button>
          <label class="question-label" for="rate">Skattad sömnkvalitét</label>
          <div class="rate">
              <input type="radio" id="star5" name="rate" value="5" />
              <label for="star5" title="text">5 stars</label>
              <input type="radio" id="star4" name="rate" value="4" />
              <label for="star4" title="text">4 stars</label>
              <input type="radio" id="star3" name="rate" value="3" />
              <label for="star3" title="text">3 stars</label>
              <input type="radio" id="star2" name="rate" value="2" />
              <label for="star2" title="text">2 stars</label>
              <input type="radio" id="star1" name="rate" value="1" />
              <label for="star1" title="text">1 star</label>
         </div>


        </div>
      </div>
<hr class="linje-ny-dagbok">

    <div class="row">
      <div class="col">
          <div class="mb-4">
            <input type="reset" value="Avbryt" class="btn btn-primary btn-avbryt">

            <input type="submit" value="Spara" class="btn btn-primary btn-spara">
          </div>


        </div>
      </div>
  </div> <!--/ Disabler -->

</form>

</div>
</div>
